        "psr-0" : {
            "callebstrom\\JPBundle" : ""
        }
    },
    "target-dir" : "callebstrom/JPBundle",
    "repositories" : [{
            "type": "vcs",
            "url": "https://github.com/callebstrom/JobProgressBundle"
    }],
    "extra" : {
    "branch-alias" : {
            "dev-master" : "some_version-dev"
        }
    }
}